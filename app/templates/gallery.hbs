<div class="row-fluid" id="gallery">
  <h3>Photo Gallery</h3>
  <button {{action 'clearGallery'}}>Delete All Photos</button>
  <ul class="thumbnails" id="gallery-list">
    {{#each controller}}
      <li>
        <img {{bindAttr src="source"}}>
        <button {{action 'deletePhoto'}}>Delete</button>
      </li>
    {{/each}}
  </ul>
</div>